
<form action = "/api/user/create" method= "post" id= "createUser">
	
  <div class="mb-3">
    <input type="text" class="form-control" placeholder= "User Name" name= "name">
	</div>


  <div class="mb-3">
    <input type="email" class="form-control" placeholder= "Email Address" name= "email">
	</div>
	
  <button type="submit" class="btn btn-primary" onClick="createUser(event)">Submit</button>
</form>
	